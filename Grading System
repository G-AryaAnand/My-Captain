 #include <bits/stdc++.h>
 using namespace std;
  int main() { 
   int marks;
   printf("Enter the marks: ");
   scanf("%d",&marks);
   if(marks<101 && marks>-1){
     if(marks>84)
     printf("Grade A");
     else if(marks>69)
     printf("Grade B");
     else if(marks>54)
     printf("Grade C");
     else if(marks>39)
     printf("Grade D");
     else
     printf("Grade F");
   }else{
     printf("wrong input given");
   }

     return 0;
}
