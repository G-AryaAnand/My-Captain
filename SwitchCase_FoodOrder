 #include <bits/stdc++.h>
 using namespace std;
  int main() { 
   int n; 
   printf("Enter the number: ");
   scanf("%d",&n);
   switch(n){
     case 1: printf("Your order-Pizza\nPrice- Rs239");break;
     case 2: printf("Your order-Burger\nPrice- Rs129");break;
     case 3: printf("Your order-Pasta\nPrice- Rs179");break;
     case 4: printf("Your order-French Fries\nPrice- Rs99");break;
     case 5: printf("Your order-Sandwich\nPrice- Rs149");break;
     default : printf("Please give a valid order");
     }

     return 0;
}
